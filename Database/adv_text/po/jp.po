msgid "adv_106102_0005_msg"
msgstr "…………"

msgid "adv_106102_0006_msg"
msgstr "レイレイ？"

msgid "adv_106102_0007_msg"
msgstr "順調といえば順調…"

msgid "adv_106102_0008_msg"
msgstr "えっと、何か気になることでもあるの？"

msgid "adv_106102_0009_msg"
msgstr "…マキナ、これを見てほしい"

msgid "adv_106102_0010_msg"
msgstr "解析結果？　どれどれ…"

msgid "adv_106102_0011_msg"
msgstr "…………！"

msgid "adv_106102_0012_msg"
msgstr "早く、カナカナたちに報告しなくちゃ！"

msgid "adv_106102_0013_msg"
msgstr "うん"

