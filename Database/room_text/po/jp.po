msgid "message_12565_txt"
msgstr "そうね＾＾"

msgid "message_12566_txt"
msgstr "みんな！\nライブ、本当にお疲れさま！"

msgid "message_12567_txt"
msgstr "気をつけてラグナに帰ってね！"

msgid "message_12568_txt"
