msgid "message_4288_txt"
msgstr "待ってるわ"

msgid "message_4289_txt"
msgstr "え！？あ、はい…"

msgid "message_4290_txt"
msgstr "じゃなくて…美雲さん！\nどういうことですか？"

msgid "message_4291_txt"
